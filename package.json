{
  "name": "udug",
  "version": "0.1.0",
  "license": "EUPL-1.2",
  "dependencies": {
    "@auth0/auth0-react": "1.10.2",
    "@entur/alert": "0.11.4",
    "@entur/button": "2.10.1",
    "@entur/dropdown": "2.3.0",
    "@entur/form": "5.4.0",
    "@entur/icons": "3.5.0",
    "@entur/layout": "2.1.8",
    "@entur/loader": "0.4.9",
    "@entur/menu": "4.1.6",
    "@entur/micro-frontend": "0.3.0",
    "@entur/modal": "1.6.6",
    "@entur/table": "4.5.17",
    "@entur/tokens": "3.4.0",
    "@entur/typography": "1.6.15",
    "@sentry/react": "7.9.0",
    "@sentry/tracing": "7.9.0",
    "husky": "7.0.4",
    "lint-staged": "12.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.0.5",
    "react-router-dom": "5.3.3",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run compile-i18n",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier src --write",
    "check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "extract-i18n": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-i18n": "formatjs compile-folder --ast lang compiled-lang"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "5.0.7",
    "@types/jest": "28.1.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "prettier": "2.7.1",
    "source-map-explorer": "2.5.2",
    "typescript": "4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
